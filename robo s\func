#include <stdio.h>
#include <stdlib.h>

int main() {

   int s1;
   int s2;
   
   int me;
   int md;

 while(1) {

 scanf ("%d %d", &s1, &s2);

     if ((s1==0) && (s2==0)) {
         me=1;
         md=1;
      printf("siga em frente");
     }
     if ((s1==1) && (s2==0)) {
         me=1;
         md=0;
      printf("vire pra direita");
    }
     if ((s1==1) && (s2==1) ) {
        me=0;
        md=0;
      printf("Pare");
     }
    if ((s1==0) && (s2==1)) {
        me=0;
        md=1;
      printf("vire pra esquerda");
     }
     
}
return 0;
}
